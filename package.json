  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest  --coverage --passWithNoTests",
    "test:unit": "cross-env NODE_ENV=test jest test/unit/* --coverage=false --passWithNoTests",
    "test:unique": "cross-env NODE_ENV=test jest --coverage=false  --passWithNoTests",
    "test:integration": "cross-env NODE_ENV=test jest test/integration/* --coverage=false --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:config": "cross-env NODE_ENV=test jest --showConfig",
    "test:clear": "cross-env NODE_ENV=test jest --clearCache",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:coverage:unit": "npm run test:unit -- --coverage",
    "test:coverage:integration": "npm run test:integration -- --coverage",
    "lint:report": "eslint --ext ts src/ -f html -o reports/lint.html",
    "format": "prettier --write src/**/*.ts",
    "format:fix": "eslint --ext ts src --fix",
    "start": "node index",
    "ts-start": "ts-node src/server.ts",
    "build": "tsc && gulp minify",
    "develop": "nodemon src/server.ts",
    "serve": "cross-env NODE_ENV=production node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cursosdevfull/NodeJS_Group06.git"
